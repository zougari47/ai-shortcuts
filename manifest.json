{
  "manifest_version": 2,
  "name": "ai-shortcuts",
  "version": "1.1",
  "description": "Use chatGPT shortcuts on Gemini and Bing Copilot.",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "96": "icons/96.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "64": "icons/64.png",
      "96": "icons/96.png",
      "128": "icons/128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://gemini.google.com/*", "https://www.bing.com/*"],
      "js": ["index.js"]
    }
  ]
}
